<div id="main">
    <div id="content">
        <!-- START cart-wrapper -->
        <div class="cart-wrapper row" data-component="pagecontext/Scope" data-scope="none" style="<%= @wish.wish_items.count == 0 ? '' : 'display: none' %>">
            <div class="container clearfix empty-cart">
                <div class="col-8">
                    <div class="container cart-head-container clearfix">
                        <div class="col-4 title-wrapper" data-scope="breadcrumbs" data-context="home;shopping cart">
                            <h1 class="checkout-title">Danh sách yêu thích</h1>
                            <h7>Không có mục nào trong danh sách yêu thích của bạn.</h7>
                        </div>
                    </div><br><br>
                    <div class="cart_empty-content">
                    </div>
                    <div class="contionue-shopping-wrapper">
                        <a href="/">
                            <button class="rbk-button-red button-primary bp-black" type="submit" value="Continue Shopping" name="dwfrm_cart_continueShopping">
                                <span>Tiếp tục mua sắm</span>
                            </button>
                        </a>
                    </div>
                </div>
            </div>
        </div>


        <!-- START cart-wrapper -->
        <div class="cart-wrapper row" style="<%= @wish.wish_items.count == 0 ? 'display: none' : '' %>">
            <div class="container ">
                <div class="col-8">
                    <!-- START products wrapper -->
                    <div class="container clearfix">
                        <div class="col-4 title-wrapper" data-scope="breadcrumbs" data-context="home;shopping cart">
                            <h1 class="checkout-title">Danh sách yêu thích</h1> 
                        </div>
                    </div>
                    <div class="container clearfix rbk-mobile-shadow-block " data-ci-test-id="cartOverview">
                        <div class="cart_table fancyform" id="dwfrm_cart">
                            <div class="co-pt_table">
                                <div class="shipment_wrapper">
                                    <% i = 0 %>
                                    <% @wish.wish_items.each do |wish_item| %>
                                    <div id="shipment_wrapper_0_<%=i%>" class="container clearfix line-item <%= i==0 ? ' first-item':''%>" style="width:80vw!important;height: 175px!important;overflow: hidden;">
                                        <% i += 1 %>
                                        <div class="imagecolumn left" style="height: 110px;width: 110px">
                                            <div class="productimg_container">
                                                <a href="/products/<%= wish_item.product.id %>?variant=<%= wish_item.variant.id %>">
                                                    <%= image_tag wish_item.variant.avatar, size: "110x110" %>
                                                </a>
                                            </div>
                                        </div>
                                        <div class="detailscolumn">
                                    
                                            <div class="product" data-id="AQ0886_550">
                                                <a class="name" href="/products/<%= wish_item.product.id %>?variant=<%= wish_item.variant.id %>" title="<%= wish_item.product.name %>"><%= wish_item.product.name %></a>
                                                <span class="label">Color: </span>
                                                <span class="value"><%= wish_item.variant.color.name.to_s.titleize %></span><br>
                                                <%= link_to 'Gỡ bỏ', wish_item_url(wish_item), method: :delete , remote: true, style: 'color:rgb(2, 134, 205);font-size: 13px;'%><br>
                                                <span class="isinstock">Trong Kho &#10003;</span>
                                            </div>
                                        </div>
                                        <div class="detailscolumn">
                                            <table style="overflow-x: scroll; overflow-y: hidden;white-space: nowrap;  width: 100%;height: 69px;display: block;">
                                            <tr>  
                                                <% wish_item.product.variants.order(:id).each do |variant| %>
                                                <td style="height: 50px; width: 50px;display: inline-block;">
                                                    <a href="/products/<%= wish_item.product.id %>?variant=<%= variant.id %>"> 
                                                    <img src="<%= url_for(variant.avatar) %>" style="margin:1px;height:50px;width:50px;cursor:pointer;<% if variant.id.to_s == wish_item.variant.id.to_s then %>border-bottom: 2px solid #000;<% end %>">
                                                    </a> 
                                                </td>
                                                <% end %>   
                                            </tr>
                                            </table>  

                                            <%= form_for wish_item.product, :url => cart_items_path, :html => { :method => :post } do |f|%>
                                            <%= f.hidden_field :variant_id, value: wish_item.variant.id %>
                                            <%= f.number_field :quantity, value: 1, class: "number-tag", min: 1 %>
                                            <%= f.hidden_field :product_id, value: wish_item.product.id %>
                                            <%= f.submit 'Thêm vào Túi', class: "btn btn-success", style: 'background:white;border:black;width:100%;padding: 0;margin:0;color:black' %>
                                            <% end %>
                                        </div>
                                    </div>
                                    <% end %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>                
            </div>
        </div>
    </div>
</div> 




